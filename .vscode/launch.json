    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch nodejs",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/apps/nestjs/src/main.ts",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "bun",
            "request": "launch",
            "name": "Launch bun",
            "program": "${workspaceFolder}/apps/bun-nestjs-pg/src/main.ts",
            "runtime": "bun"
        }
    ]
}